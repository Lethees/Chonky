{
  "name": "chonky",
  "version": "2.2.0",
  "homepage": "https://chonky.io/",
  "author": {
    "name": "Timur Kuzhagaliyev",
    "email": "tim.kuzh@gmail.com",
    "url": "https://foxypanda.me/"
  },
  "keywords": [
    "file",
    "chonky",
    "file-dialog",
    "file-browser",
    "react",
    "react-component"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimboKZ/Chonky.git"
  },
  "bugs": {
    "url": "https://github.com/TimboKZ/Chonky/issues"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/chonky.esm.js",
  "size-limit": [
    {
      "path": "dist/chonky.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/chonky.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/addons": "^6.1.17",
    "@storybook/react": "^6.1.17",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "babel-loader": "^8.2.2",
    "husky": "^5.0.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "size-limit": "^4.9.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.4"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/react-dnd": "^3.0.2",
    "@types/react-redux": "^7.1.16",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/shortid": "^0.0.29",
    "exact-trie": "^1.0.13",
    "fast-sort": "^2.2.0",
    "filesize": "^6.1.0",
    "fuzzy-search": "^3.2.1",
    "hotkeys-js": "^3.8.2",
    "memoizee": "^0.4.15",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-intl": "^5.12.2",
    "react-jss": "^10.5.1",
    "react-redux": "^7.2.2",
    "react-virtualized-auto-sizer": "^1.0.4",
    "react-window": "^1.8.6",
    "redux-watch": "^1.2.0",
    "shortid": "^2.2.16",
    "tsdef": "^0.0.14"
  }
}
